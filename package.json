{
  "name": "netflix-ms",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "NODE_ENV=local nest start",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:debug": "NODE_ENV=local nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "prisma generate && sh setupHusky.sh",
    "db:seed": "prisma db seed",
    "db:up": "docker-compose down --rmi all -v --remove-orphans && docker-compose up -d && dotenv -e local.env prisma db push",
    "db:down": "docker-compose down --rmi all -v --remove-orphans"
  },
  "dependencies": {
    "@nestjs/common": "9.3.9",
    "@nestjs/core": "9.3.9",
    "@nestjs/jwt": "10.0.2",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/swagger": "6.2.1",
    "@prisma/client": "4.11.0",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@nestjs/cli": "9.2.0",
    "@nestjs/config": "2.3.1",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.9",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "dotenv-cli": "7.1.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "13.2.0",
    "prisma": "4.11.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "pnpm lint:fix",
      "pnpm test -- --findRelatedTests --coverageThreshold --passWithNoTests '{}'"
    ]
  },
  "pnpm": {
    "overrides": {
      "webpack@>=5.0.0 <5.76.0": ">=5.76.0"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
